import telebot

# ضع هنا مفتاح API الخاص بالبوت الخاص بك
API_TOKEN = "1687364449:AAGai4ClSHNo2Di5sKyvc_PudTVYRfBsmC4"

# إنشاء كائن البوت
bot = telebot.TeleBot(API_TOKEN)

# معالجة رسالة النصوص
@bot.message_handler(func=lambda message: True)
def echo_all(message):
    # إرسال نفس النص المستلم كإجابة
    bot.reply_to(message, f"لقد أرسلت: {message.text}")

# بدء تشغيل البوت
print("البوت يعمل الآن...")
bot.polling()
